{
	"name": "@omni-tools/map-age-cleaner",
	"version": "1.0.0-alpha.0",
	"description": "Automatically cleanup expired items in a Map",
	"license": "MIT",
	"repository": "omni-tools/amnesia",
	"author": {
		"name": "Sam Verschueren",
		"email": "sam.verschueren@gmail.com",
		"url": "github.com/SamVerschueren"
	},
	"main": "dist/index.js",
	"engines": {
		"node": ">=6"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepublishOnly": "npm run build",
		"pretest": "npm run build -- --sourceMap",
		"test": "npm run lint && nyc ava dist/test.js",
		"generate-coverage-report": "nyc report --reporter=text-lcov > .nyc_output/coverage.lcov",
		"lint": "tslint --format stylish --project .",
		"build": "npm run clean && tsc",
		"clean": "del-cli dist"
	},
	"files": [
		"dist/index.js",
		"dist/index.d.ts"
	],
	"keywords": [
		"map",
		"age",
		"cleaner",
		"maxage",
		"expire",
		"expiration",
		"expiring"
	],
	"dependencies": {
		"@types/debug": "^4.1.5",
		"debug": "^4.1.1",
		"p-defer": "^3.0.0"
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "^0.1.3",
		"@types/delay": "^3.1.0",
		"@types/node": "^12.7.4",
		"ava": "^2.3.0",
		"codecov": "^3.5.0",
		"del-cli": "^3.0.0",
		"delay": "^4.3.0",
		"nyc": "^14.1.1",
		"tslint": "^5.20.0",
		"tslint-xo": "^0.16.0",
		"typescript": "^3.6.2"
	},
	"typings": "dist/index.d.ts",
	"sideEffects": false,
	"nyc": {
		"extends": "@istanbuljs/nyc-config-typescript",
		"all": true,
		"exclude": [
			"dist/test*"
		]
	}
}
